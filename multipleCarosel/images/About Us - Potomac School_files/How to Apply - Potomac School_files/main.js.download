// ================================
// SUMMER SITE
// ================================


jQuery(function($) {

  var $body = 'body';
  var $drawer = '.drawer';

  //$($drawer).insertBefore('#fsPageWrapper');

  if($('body.summer-home:not(.fsComposeMode)').length) {

      $('#fsHeader').prepend('<span id="bird"></span>');
    }

  if($('body.summer:not(.fsComposeMode)').length) {

      $('#fsHeader').prepend('<div class="paper-top"><div class="paper-top-inner"><a href="/" class="back-to-potomac">Back to Potomac</a></div></div><span id="bird"></span>');
      $('#fsPageWrapper').wrapInner('<div id="fsPageWrapperInner"></div>');

      $('#fsPoweredByFinalsite').appendTo('#fsFooter');
      $('#fsFooter .fsBanner').append('<span id="tree-left"></span><span id="tree-right"></span>');

      $('.fs_style_33').each(function(){
          $(this).wrap('<div class="summer-image"></div>');
      });

  // Create a section title based on the current page

  var sectionTitle = $('.nav-summer .fsNavLevel1 > .fsNavCurrentPage > a').text();
  var sectionTitle2 = $('.nav-summer .fsNavLevel1 > .fsNavCurrentPageAncestor > a').text();

  $('.nav-sub').prepend('<div class="section-title">' + sectionTitle + sectionTitle2 + '</div>');
  $('.section-title:empty').remove();

  // If the section title is clicked, activate the sub nav
  $('.section-title').click(function() {

    $(this).parent().toggleClass('active-nav');

  });

  } // End Summer

  // ================================
  // Flexible Structure
  // ================================

  // Run mobile first changes
  bpBelowSummerMedium();

  enquire
    .register("screen and (min-width: 950px)", {
      match: function() {
        bpSummerMedium();
      },
      unmatch: function() {
        bpBelowSummerMedium();
      }
    }, true);

// What would you like to do under your defined breakpoint
function bpBelowSummerMedium() {

$('.fsSection266 .nav-summer').prependTo($drawer);

 if (!$('.drawer #summer-reg-link').length ) {
    $('.summer #summer-reg-link').clone().insertAfter('.drawer .nav-summer');
 }

}

